	xmlns:ns1="http://www.w3.org/2001/XMLSchema">

	<!-- Import the client WSDL -->
	<bpel:import namespace="http://wsBoutique.ws.mylogos.top"
		location="WSBoutique.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
	<bpel:import namespace="http://wsFacturation.service.mylogos.top"
		location="WSFacturation.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
	<bpel:import namespace="http://wsbanque.service.mylogos.top"
		location="WSBanque.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"></bpel:import>
	<bpel:import location="bpelprocessArtifacts.wsdl"
		namespace="http://orchestrateur.mylogos.top" importType="http://schemas.xmlsoap.org/wsdl/" />

	<!-- ================================================================= -->
	<!-- PARTNERLINKS -->
	<!-- List of services participating in this BPEL process -->
	<!-- ================================================================= -->
	<bpel:partnerLinks>
		<!-- The 'client' role represents the requester of this service. -->
		<bpel:partnerLink name="client" partnerLinkType="tns:bpelprocess"
			myRole="bpelprocessProvider" />
		<bpel:partnerLink name="PartnerLinkBanque"
			partnerLinkType="tns:Paiement" partnerRole="payer"></bpel:partnerLink>
		<bpel:partnerLink name="PartnerLinkFacturation"
			partnerLinkType="tns:Facturation" partnerRole="facturer"></bpel:partnerLink>
		<bpel:partnerLink name="PartnerLinkBoutique"
			partnerLinkType="tns:Transfert" partnerRole="transferer"></bpel:partnerLink>
	</bpel:partnerLinks>

	<!-- ================================================================= -->
	<!-- VARIABLES -->
	<!-- List of messages and XML documents used within this BPEL process -->
	<!-- ================================================================= -->
	<bpel:variables>
		<!-- Reference to the message passed as input during initiation -->
		<bpel:variable name="input" messageType="tns:bpelprocessRequestMessage" />

		<!-- Reference to the message that will be returned to the requester -->
		<bpel:variable name="output" messageType="tns:bpelprocessResponseMessage" />
		<bpel:variable name="PartnerLinkBanqueResponse"
			messageType="ns:payerResponse"></bpel:variable>
		<bpel:variable name="PartnerLinkBanqueRequest"
			messageType="ns:payer"></bpel:variable>
		<bpel:variable name="PartnerLinkFacturationResponse"
			messageType="ns0:facturerResponse"></bpel:variable>
		<bpel:variable name="PartnerLinkFacturationRequest"
			messageType="ns0:facturer"></bpel:variable>
		<bpel:variable name="ResultatFactureFinale" type="ns1:string"></bpel:variable>
	</bpel:variables>

	<!-- ================================================================= -->
	<!-- ORCHESTRATION LOGIC -->
	<!-- Set of activities coordinating the flow of messages across the -->
	<!-- services integrated within this business process -->
	<!-- ================================================================= -->
	<bpel:sequence name="main">

		<!-- Receive input from requester. Note: This maps to operation defined 
			in bpelprocess.wsdl -->
		<bpel:receive name="receiveInput" partnerLink="client"
			portType="tns:bpelprocess" operation="process" variable="input"
			createInstance="yes" />

		<!-- Generate reply to synchronous request -->
		<bpel:assign validate="no" name="AssignDonneeBancaire">
			<bpel:copy>
				<bpel:from>
					<bpel:literal>
						<tns:payer xmlns:tns="http://wsbanque.service.mylogos.top"
							xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
							<paiement>paiement</paiement>
						</tns:payer>
					</bpel:literal>
				</bpel:from>
				<bpel:to variable="PartnerLinkBanqueRequest" part="parameters"></bpel:to>
			</bpel:copy>
			<bpel:copy>
				<bpel:from part="payload" variable="input">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:informationBancaire]]></bpel:query>
				</bpel:from>
				<bpel:to part="parameters" variable="PartnerLinkBanqueRequest">
					<bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[paiement]]></bpel:query>
				</bpel:to>
			</bpel:copy>
		</bpel:assign>
		<bpel:invoke name="InvokeBanque" partnerLink="PartnerLinkBanque"
			operation="payer" inputVariable="PartnerLinkBanqueRequest"
			outputVariable="PartnerLinkBanqueResponse"></bpel:invoke>
		<bpel:if name="If">
			<bpel:condition expressionLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[$PartnerLinkBanqueResponse.parameters/retourPaiement = '{"ok":true}']]></bpel:condition>
            <bpel:sequence name="SequenceFacturation"><bpel:assign validate="no" name="AssignFacture">
                    
                    
                    
                    <bpel:copy>
                        <bpel:from><bpel:literal><tns:facturer xmlns:tns="http://wsFacturation.service.mylogos.top" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <commande>commande</commande>
  <utilisateur>utilisateur</utilisateur>
</tns:facturer>
</bpel:literal></bpel:from>
                        <bpel:to variable="PartnerLinkFacturationRequest" part="parameters"></bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from part="payload" variable="input">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:commande]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="parameters" variable="PartnerLinkFacturationRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[commande]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                    <bpel:copy>
                        <bpel:from part="payload" variable="input">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:utilisateur]]></bpel:query>
                        </bpel:from>
                        <bpel:to part="parameters" variable="PartnerLinkFacturationRequest">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[utilisateur]]></bpel:query>
                        </bpel:to>
                    </bpel:copy>
                </bpel:assign>
                <bpel:invoke name="InvokeFacturation" partnerLink="PartnerLinkFacturation" operation="facturer" inputVariable="PartnerLinkFacturationRequest" outputVariable="PartnerLinkFacturationResponse"></bpel:invoke>
                <bpel:assign validate="no" name="AssignResultatFacture">
                    <bpel:copy>
                        <bpel:from part="parameters" variable="PartnerLinkFacturationResponse">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[resultFacturation]]></bpel:query>
                        </bpel:from>
                        <bpel:to variable="ResultatFactureFinale"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
            
            </bpel:sequence>
            <bpel:else>
                <bpel:assign validate="no" name="AssignEchecPaiement">
                    <bpel:copy>
                        <bpel:from part="parameters" variable="PartnerLinkBanqueResponse">
                            <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[retourPaiement]]></bpel:query>
                        </bpel:from>
                        <bpel:to variable="ResultatFactureFinale"></bpel:to>
                    </bpel:copy>
                </bpel:assign>
            </bpel:else>
        </bpel:if>
        <bpel:assign validate="no" name="AssignOutput">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:bpelprocessResponse xmlns:tns="http://orchestrateur.mylogos.top" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:bpelprocessResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from variable="ResultatFactureFinale">
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:bpelprocess"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

